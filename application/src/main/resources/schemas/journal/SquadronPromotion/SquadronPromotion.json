{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "extends" : { "$ref": "../_Event.json"},
  "allOf": [{ "$ref": "../_Event.json"}],
  "properties" : {
    "SquadronID" : {
      "title" : "SquadronID",
      "type" : "integer",
      "examples" : [ 3, 0, 4 ]
    },
    "SquadronName" : {
      "title" : "SquadronName",
      "type" : "string",
      "examples" : [ "TestSquadron" ]
    },
    "OldRank" : {
      "title" : "OldRank",
      "type" : "integer",
      "examples" : [ 2 ]
    },
    "NewRank" : {
      "title" : "NewRank",
      "type" : "integer",
      "examples" : [ 3 ]
    },
    "OldRankName" : {
      "title" : "OldRankName",
      "type" : "string",
      "examples" : [ "$Squadron_DefaultRankName_Rank4;" ]
    },
    "OldRankName_Localised" : {
      "title" : "OldRankName_Localised",
      "type" : "string",
      "examples" : [ "Rookie" ]
    },
    "NewRankName" : {
      "title" : "NewRankName",
      "type" : "string",
      "examples" : [ "$Squadron_DefaultRankName_Rank1;" ]
    },
    "NewRankName_Localised" : {
      "title" : "NewRankName_Localised",
      "type" : "string",
      "examples" : [ "Senior Officer" ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "SquadronName", "OldRank", "NewRank" ]
}