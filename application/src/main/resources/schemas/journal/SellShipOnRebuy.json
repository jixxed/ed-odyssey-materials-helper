{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "When written: When selling a stored ship to raise funds when on insurance/rebuy screen",
  "extends" : { "$ref": "Event.json" },
  "properties" : {
    "ShipType" : {
      "title" : "ShipType",
      "type" : "string",
      "examples" : [ "Dolphin" ]
    },
    "System" : {
      "title" : "System",
      "type" : "string",
      "examples" : [ "Shinrarta Dezhra" ]
    },
    "SellShipId" : {
      "title" : "SellShipId",
      "type" : "integer",
      "examples" : [ 4 ]
    },
    "ShipPrice" : {
      "title" : "ShipPrice",
      "type" : "integer",
      "examples" : [ 4110183 ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "ShipType", "System", "SellShipId", "ShipPrice" ]
}