{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "extends" : { "$ref": "Event.json" },
  "properties" : {
    "Success" : {
      "title" : "Success",
      "type" : "boolean",
      "examples" : [ true ]
    },
    "Submitted" : {
      "title" : "Submitted",
      "type" : "boolean",
      "examples" : [ true ]
    },
    "Interdicted" : {
      "title" : "Interdicted",
      "description": "Victim pilot name",
      "type" : "string",
      "examples" : [ "Fred Flintstone" ]
    },
    "IsPlayer" : {
      "title" : "IsPlayer",
      "description": "Whether player or npc",
      "type" : "boolean",
      "examples" : [ true ]
    },
    "CombatRank" : {
      "title" : "CombatRank",
      "description": "If a player",
      "type" : "integer",
      "examples" : [ 5 ]
    },
    "Faction" : {
      "title" : "Faction",
      "description": "If npc",
      "type" : "string",
      "examples" : [ "Royal Phoenix Corporation", "The Ant Hill Mob", "LFT 926 Blue Hand Gang" ]
    },
    "Power" : {
      "title" : "Power",
      "description": "If npc is working for a power",
      "type" : "boolean",
      "examples" : [ false, true ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "Success", "Interdicted", "IsPlayer" ]
}