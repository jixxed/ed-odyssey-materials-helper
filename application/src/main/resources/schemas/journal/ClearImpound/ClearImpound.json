{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "extends" : { "$ref": "../_Event.json"},
  "allOf": [{ "$ref": "../_Event.json"}],
  "properties" : {
    "ShipType" : {
      "title" : "ShipType",
      "type" : "string",
      "examples" : [ "asp", "anaconda", "empire_courier" ]
    },
    "ShipType_Localised" : {
      "title" : "ShipType_Localised",
      "type" : "string",
      "examples" : [ "Asp Explorer", "Imperial Courier" ]
    },
    "ShipID" : {
      "title" : "ShipID",
      "type" : "integer",
      "examples" : [ 34, 7, 44 ]
    },
    "ShipMarketID" : {
      "title" : "ShipMarketID",
      "type" : "integer",
      "examples" : [ 3705689344, 3706338816 ]
    },
    "MarketID" : {
      "title" : "MarketID",
      "type" : "integer",
      "examples" : [ 3705689344, 3706338816 ]
    },
    "System" : {
      "title" : "System",
      "type" : "string",
      "examples" : [ "Tascheter Sector AV-Y b5" ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "ShipType", "ShipID", "ShipMarketID", "MarketID" ]
}