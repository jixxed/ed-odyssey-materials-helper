{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "When written: at startup, if part of a squadron",
  "extends" : { "$ref": "../_Event.json"},
  "allOf": [{ "$ref": "../_Event.json"}],
  "properties" : {
    "SquadronName" : {
      "title" : "SquadronName",
      "type" : "string",
      "examples" : [ "TestSquadron" ]
    },
    "CurrentRank" : {
      "title" : "CurrentRank",
      "type" : "integer",
      "examples" : [ 3, 0, 4 ]
    },
    "CurrentRankName" : {
      "title" : "CurrentRankName",
      "type" : "string",
      "examples" : [ "Pilot", "$Squadron_DefaultRankName_Rank0;" ]
    },
    "CurrentRankName_Localised" : {
      "title" : "CurrentRankName_Localised",
      "type" : "string",
      "examples" : [ "Pilot" ]
    },
    "SquadronID" : {
      "title" : "SquadronID",
      "type" : "integer",
      "examples" : [ 3, 0, 4 ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "SquadronName", "CurrentRank" ]
}