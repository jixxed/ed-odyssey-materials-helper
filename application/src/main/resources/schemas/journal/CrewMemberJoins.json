{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "When written: When another player joins your ship's crew",
  "extends" : { "$ref": "Event.json" },
  "properties" : {
    "Telepresence" : {
      "title" : "Telepresence",
      "description": "only from Odyssey build",
      "type" : "boolean",
      "examples" : [ false ]
    },
    "Crew" : {
      "title" : "Crew",
      "description": "Player's commander name",
      "type" : "string",
      "examples" : [ "Alice", "Bob" ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "Crew" ]
}