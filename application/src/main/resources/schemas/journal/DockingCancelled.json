{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "When written: when the player cancels a docking request",
  "extends" : { "$ref": "Event.json" },
  "properties" : {
    "MarketID" : {
      "title" : "MarketID",
      "type" : "integer",
      "examples" : [ 3229338368, 3224091904, 3705689344 ]
    },
    "StationName" : {
      "title" : "StationName",
      "type" : "string",
      "examples" : [ "Thoreau Station", "Poisson Plant", "A1A-B2B" ]
    },
    "StationType" : {
      "title" : "StationType",
      "type" : "string",
      "examples" : [ "Outpost", "FleetCarrier", "Coriolis" ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "MarketID", "StationName", "StationType" ]
}