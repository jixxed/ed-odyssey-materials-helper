{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "When written: when a player defects from one power to another",
  "extends" : { "$ref": "Event.json" },
  "properties" : {
    "FromPower" : {
      "title" : "FromPower",
      "type" : "string",
      "examples" : [ "Aisling Duval" ]
    },
    "ToPower" : {
      "title" : "ToPower",
      "type" : "string",
      "examples" : [ "Li Yong-Rui" ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "FromPower", "ToPower" ]
}