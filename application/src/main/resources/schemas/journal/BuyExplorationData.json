{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "When Written: when buying system data via the galaxy map",
  "extends" : { "$ref": "Event.json" },
  "properties" : {
    "System" : {
      "title" : "System",
      "type" : "string",
      "examples" : [ "Acokwus", "BD-17 1955", "HIP 90467" ]
    },
    "Cost" : {
      "title" : "Cost",
      "type" : "integer",
      "examples" : [ 4850, 1762, 153 ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "System", "Cost" ]
}