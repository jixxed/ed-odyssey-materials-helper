{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "When written: when selling a module in storage at another station",
  "extends" : { "$ref": "Event.json" },
  "properties" : {
    "StorageSlot" : {
      "title" : "StorageSlot",
      "type" : "integer",
      "examples" : [ 6, 54, 9 ]
    },
    "SellItem" : {
      "title" : "SellItem",
      "type" : "string",
      "examples" : [ "$int_hyperdrive_size3_class1_name;", "$int_hyperdrive_size5_class1_name;", "$int_lifesupport_size4_class1_name;" ]
    },
    "SellItem_Localised" : {
      "title" : "SellItem_Localised",
      "type" : "string",
      "examples" : [ "FSD", "Life Support", "Power Distributor" ]
    },
    "ServerId" : {
      "title" : "ServerId",
      "type" : "integer",
      "examples" : [ 128064108, 128064118, 128064153 ]
    },
    "SellPrice" : {
      "title" : "SellPrice",
      "type" : "integer",
      "examples" : [ 6114, 63012, 11065 ]
    },
    "Ship" : {
      "title" : "Ship",
      "type" : "string",
      "examples" : [ "empire_trader", "python", "federation_corvette" ]
    },
    "ShipID" : {
      "title" : "ShipID",
      "type" : "integer",
      "examples" : [ 11, 27, 29 ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "StorageSlot", "SellItem", "SellItem_Localised", "ServerId", "SellPrice", "Ship", "ShipID" ]
}