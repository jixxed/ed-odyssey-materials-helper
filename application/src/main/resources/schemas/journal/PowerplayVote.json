{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "When written: when voting for a system expansion",
  "extends" : { "$ref": "Event.json" },
  "properties" : {
    "Power" : {
      "title" : "Power",
      "type" : "string",
      "examples" : [ "Aisling Duval" ]
    },
    "Votes" : {
      "title" : "Votes",
      "type" : "integer",
      "examples" : [ 1 ]
    },
    "VoteToConsolidate" : {
      "title" : "VoteToConsolidate",
      "type" : "integer",
      "examples" : [ 1 ]
    },
    "System" : {
      "title" : "System",
      "description": "Documented, but appears to be unused.",
      "type" : "string",
      "examples" : [ "LFT 926", "Shinrarta Dezhra", "Cloomeia FG-Y e95" ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "Power", "Votes", "VoteToConsolidate" ]
}