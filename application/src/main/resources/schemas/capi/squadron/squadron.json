{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CapiSquadron",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "tag": {
      "type": "string"
    },
    "ownerId": {
      "type": "integer"
    },
    "platform": {
      "type": "string"
    },
    "created": {
      "type": "string"
    },
    "acceptingNewMembers": {
      "type": "integer"
    },
    "powerId": {
      "type": [
        "integer",
        "null"
      ]
    },
    "powerName": {
      "type": "string"
    },
    "superpowerId": {
      "type": "integer"
    },
    "superpowerName": {
      "type": "string"
    },
    "factionId": {
      "type": "integer",
      "description": "0 if none"
    },
    "factionName": {
      "type": "string",
      "description": "None if none"
    },
    "factionHomeSystemId": {
      "type": ["integer", "null"],
      "description": "null if none"
    },
    "factionHomeSystemName": {
      "type": ["string", "null"],
      "description": "null if none"
    },
    "userTags": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "memberCount": {
      "type": "integer"
    },
    "full": {
      "type": "integer"
    },
    "description": {
      "type": "string"
    },
    "motto": {
      "type": "string"
    },
    "isMentoring": {
      "type": "boolean"
    },
    "perks": {
      "type": "object",
      "properties": {
        "primary": {
          "type": "string"
        },
        "secondary": {
          "type": "string"
        },
        "cooldowns": {
          "type": "object",
          "properties": {
            "primary": {
              "type": ["integer", "null"],
              "description": "null if initial"
            },
            "secondary": {
              "type": ["integer", "null"],
              "description": "null if initial"
            }
          },
          "required": [
            "primary",
            "secondary"
          ]
        }
      },
      "required": [
        "primary",
        "secondary",
        "cooldowns"
      ]
    },
    "active7Days": {
      "type": "integer"
    },
    "active30Days": {
      "type": "integer"
    },
    "squadronCarrier": {
      "type": "object",
      "$ref": "../fleetcarrier/fleetcarrier.json"
    },
    "members": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "squadron_id": {
            "type": "integer"
          },
          "member_id": {
            "type": "integer"
          },
          "joined": {
            "type": "string"
          },
          "expires_ts": {
            "type": "integer"
          },
          "status": {
            "type": "string"
          },
          "requested": {
            "type": "string"
          },
          "requested_ts": {
            "type": "integer"
          },
          "rank_id": {
            "type": "integer"
          },
          "requestletter": {
            "type": [
              "string",
              "null"
            ]
          },
          "credit_contributions": {
            "type": "integer"
          },
          "presence": {
            "type": "boolean"
          },
          "biography": {
            "type": "string"
          },
          "avatar": {
            "type": "string"
          },
          "user_id": {
            "type": "integer"
          },
          "rankCombat": {
            "type": "integer"
          },
          "rankExplore": {
            "type": "integer"
          },
          "rankTrade": {
            "type": "integer"
          },
          "rankCqc": {
            "type": "integer"
          },
          "soldierRank": {
            "type": "integer"
          },
          "exobiologistRank": {
            "type": "integer"
          },
          "empireRank": {
            "type": "integer"
          },
          "federationRank": {
            "type": "integer"
          },
          "powerRank": {
            "type": [
              "integer",
              "null"
            ]
          },
          "powerId": {
            "type": [
              "integer",
              "null"
            ]
          },
          "lastOnline": {
            "type": "string"
          },
          "lastOnline_ts": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "lastLocationId": {
            "type": ["integer", "null"]
          },
          "lastLocation": {
            "type": "string"
          },
          "shipName": {
            "type": "string"
          },
          "shipModel": {
            "type": "string"
          },
          "userPlayTime": {
            "type": "integer"
          },
          "systemAddr": {
            "type": "integer"
          },
          "avatarXML": {
            "type": "string"
          }
        },
        "required": [
          "squadron_id",
          "member_id",
          "joined",
          "expires_ts",
          "status",
          "requested",
          "requested_ts",
          "rank_id",
          "credit_contributions",
          "presence",
          "biography",
          "user_id",
          "rankCombat",
          "rankExplore",
          "rankTrade",
          "rankCqc",
          "soldierRank",
          "exobiologistRank",
          "empireRank",
          "federationRank",
          "lastOnline",
          "lastOnline_ts",
          "name",
          "lastLocation",
          "shipName",
          "shipModel",
          "userPlayTime",
          "systemAddr"
        ]
      }
    },
    "bank": {
      "type": ["object", "null"],
      "properties": {
        "credits": {
          "type": "object",
          "existingJavaType": "java.util.Map<String,BankItem[]>"
        },
        "commodities": {
          "type": "object",
          "existingJavaType": "java.util.Map<String,BankItem[]>"
        },
        "microresources": {
          "type": "object",
          "existingJavaType": "java.util.Map<String,BankItem[]>"
        },
        "ships": {
          "type": "object",
          "existingJavaType": "java.util.Map<String,ShipItem[]>"
        }
      },
      "required": [
        "credits"
      ]
    }
  },
  "required": [
    "id",
    "name",
    "tag",
    "ownerId",
    "platform",
    "created",
    "acceptingNewMembers",
    "powerId",
    "powerName",
    "superpowerId",
    "superpowerName",
    "factionId",
    "factionName",
    "factionHomeSystemId",
    "factionHomeSystemName",
    "userTags",
    "memberCount",
    "full",
    "description",
    "motto",
    "isMentoring",
    "perks",
    "active7Days",
    "active30Days",
    "members",
    "bank"
  ]
}